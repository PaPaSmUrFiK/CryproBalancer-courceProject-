<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="412.0" prefWidth="394.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Flights">
   <children>
      <TableView fx:id="tableFlights" layoutX="21.0" layoutY="69.0" onMouseClicked="#OnMouseClicked" prefHeight="288.0" prefWidth="353.0">
        <columns>
          <TableColumn fx:id="Route" prefWidth="75.0" text="Route" />
          <TableColumn fx:id="Aircraft" prefWidth="75.0" text="Aircraft" />
            <TableColumn fx:id="DepartureTime" prefWidth="127.0" text="DepartureTime" />
            <TableColumn fx:id="Status" prefWidth="75.0" text="Status" />
        </columns>
      </TableView>
      <Button fx:id="buttonDetails" disable="true" layoutX="322.0" layoutY="373.0" mnemonicParsing="false" onAction="#OnDetails" text="Детали" />
      <Button fx:id="buttonAdd" layoutX="38.0" layoutY="31.0" mnemonicParsing="false" onAction="#OnAdd" prefHeight="25.0" prefWidth="79.0" text="Добавить" visible="false" />
      <Button fx:id="buttonDelete" disable="true" layoutX="158.0" layoutY="31.0" mnemonicParsing="false" onAction="#OnDelete" prefHeight="25.0" prefWidth="79.0" text="Удалить" visible="false" />
      <Button fx:id="buttonEdit" disable="true" layoutX="283.0" layoutY="31.0" mnemonicParsing="false" onAction="#OnEdit" prefHeight="25.0" prefWidth="79.0" text="Редактировать" visible="false" />
      <Button fx:id="btnLogout" layoutX="257.0" layoutY="373.0" mnemonicParsing="false" onAction="#OnLogout" text="Выход" />
      <Button fx:id="btnDiagram" layoutX="21.0" layoutY="373.0" mnemonicParsing="false" onAction="#OnDiagram" text="Диаграмма" />
   </children>
</AnchorPane>
